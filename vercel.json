{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_VERSION": "18"
  },
  "github": {
    "silent": true
  },
  "gitlab": {
    "silent": true
  },
  "bitbucket": {
    "silent": true
  }
}
