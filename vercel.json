{
  "redirects": [
    {
      "source": "https://tacosborabora.com",
      "destination": "https://www.tacosborabora.com",
      "permanent": true
    },
    {
      "source": "http://tacosborabora.com",
      "destination": "https://www.tacosborabora.com",
      "permanent": true
    },
    {
      "source": "/ordenar",
      "destination": "/ordenar",
      "permanent": false
    }
  ],
  "routes": [
    {
      "src": "/ordenar",
      "dest": "/ordenar",
      "continue": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
